<h1>Listing campaigns</h1>
<h3>Errors <%= Vote.import_errors %></h3>

<table>
  <tr>
    <th>Name</th>
    <th>Errors</th>
    <th></th>
    <th></th>
  </tr>

<% @campaigns.each do |campaign| %>
  <tr>
    <td><%= campaign.name %></td>
    <td><%= campaign.overal_errors %></td>
    <td><%= link_to 'Show', campaign %></td>
  </tr>
<% end %>
</table>

